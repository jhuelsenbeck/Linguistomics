using Format, WorldLanguages;
//======================================================================

class Leaf: NodeClass {
  var LanguageClass Language;

  Constructor(LanguageClass language, branch=1.0) {
    super.Constructor(language?.ShortName);
    Label        = language;
    BranchLength = branch;
    Language     = language;
  }
}

class Branch: NodeClass {
  Constructor(branch=1.0) {
    super.Constructor(null);
    BranchLength = branch;
  }
}

