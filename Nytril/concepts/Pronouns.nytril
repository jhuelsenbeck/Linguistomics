with Concepts.We {
  Done = true;

  with Primary {
    Latin            = "noː-s";                // nos
    French           = "nu--";                 // nous
    Spanish          = "no-s";                 // nos (dative, accusative and reflexive of nosotros)
    Italian          = "noːi-";                // noi MORPHOLOGICAL CHANGE?
    PortugueseBrazil = "nɔj-s";                // nós nɔ(j)s DIPHTHONG
    Portuguese       = "nɔ-ʃ";                 // nós
    Catalan          = Empty;                  // no cognate (Old. Cat. nos "nus")
    Walloon          = "no--";                 // nos; Alt. ns "n" before consonant, "nz" before vowel, "gn" or "nn" after a verb
    Friulian         = "noː--";                // nô
    Romanian         = "noj-";                 // noi DIPHTHONG
  }
}

