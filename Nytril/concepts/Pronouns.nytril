with Concepts.I {
  Done = true;

  with Primary {
    Latin            = "-eɡoː";                // ego; all Romance forms from *eo. initial C all continue an epenthetic consonant?
    French           = "ʒə--";                 // je
    Spanish          = "ʝ--o";                 // yo
    Italian          = "i--o";                 // io
    PortugueseBrazil = "-e-w";                 // eu
    Portuguese       = "-e-w";                 //
    Catalan          = "ʒ--ɔ";                 // jo
    Walloon          = "ʤɪ--";                 //
    Friulian         = "j--o";                 // jo (accented pronoun), Alt. ["o"] ⟨o⟩ (clitic pronoun) ALIGNMENT
    Romanian         = "je-w";                 // eu Alt. pronounciations ["jo"; "eu"] ALIGNMENT
  }
}

