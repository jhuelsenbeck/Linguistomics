  //======================================================================

  let HasFlag(flags, f)       = (flags & f) != None
  let NotFlag(flags, f)       = (flags & f) == None
  let HasMask(flags, mask, f) = (flags & mask) == (f & mask)

  //======================================================================

  let SoundColumns = 4
  let SoundSize    = 40 pts

  let ShowSampa(sampa) = Span {
    Style.MonoFamily,
    sampa,
  }

  let SoundName(sound) = Span {
    if (sound.Flags.HasFlag(Vowel))
      sound.Open,
      Space,
      sound.Backness,
    else
      sound.Place,
    end,
    Space,
    Span {
      Separator: Space,
      if (sound.Flags.HasFlag(Diacritic))
        sound.Description
      else
        each sound.Flags,
      end
    }
  }

  let ShowCodePoint(c) = {Style.MonoFamily, TextRadix: 16, TextDigits: 4} Type.Integer(c)

  let CodePoints(text) = Span {
    Separator: Lang.Separator,
    if (text)
      ShowCodePoint(each text)
    end
  }

  let SoundDisplay(sound, location) = Block {
    ParAlignment: ParAlignments.Center,
    Paragraph {
      LocationMark: location,
      SpaceBefore: 8 pts,
      SpaceAfter: 8 pts,
      TextHeight: SoundSize,
      Style.IPAFamily,
      SpaceAfter: SoundSize * 0.125,
      sound.Text,
    },
    Paragraph {
      TextHeight: 10 pts,
      SoundName(sound),
    },
    Paragraph {
      TextHeight: 8 pts,
      ParBackground: 95%,
      CodePoints(sound.Text),
    },
    Paragraph {
      TextHeight: 8 pts,
      if (sound.Sampa and sound.Sampa.Length > 0)
        ParBackground: 90%,
        ShowSampa(sound.Sampa)
      else
        if (sound.Text.Length > 0)
          ParBackground: Colors.Red,
//        Assert(false, "Missing Sampa definition"),
        end
      end
    },
  }

  let SoundCell(ref sound) = Cell {
    Edge: 0.25 pts {Color: Colors.LightGray},
    Padding: 2 pts,
    SoundDisplay(sound, sound.FullSymbolName)
  }

  let SoundRow(sounds) = Row {
    SoundCell(each sounds)
  }

  let SoundTable(sounds) = Block {
    Table {
      Columns: [Metrics.Content.Width / SoundColumns] * SoundColumns,
      Style.TitleBar(Lang.IPAListing, SoundColumns),
      SoundRow(each (sounds / SoundColumns))
    },
    Style.TableNotes
  }

  let SoundPopup(sound) = Frame {
    Width: 2 inches,
    SoundDisplay(sound, null)
  }

  let CharacterPopup(word) = Frame {
    Width: 2 inches,
    TextHeight: 14 pts,
    ParAlignment: ParAlignments.Center,
    Paragraph {
      ShowIPA(word),
      SpaceAfter: 8 pts,
    },
    Paragraph {
      TextColor: Colors.Red,
      "\"",
      ShowSampa(word.Sampa),
      "\"",
    },
  }

  let ShowIPA(word) = Span {
    (each WordToSounds(word)).Text
  }

  let ShowSound(ref sound, flags=Impossible) = Span {
    Style.IPAFamily,
    if (sound.Flags.NotFlag(Punctuation))
      Popup: SoundPopup.Call(sound),
      Link: sound.FullSymbolName,
    end,
    if (sound.Flags.HasFlag(flags))
      Assert(false, "Impossible sound"),
      TextColor: Colors.Red,
    end,
    if (sound.Flags.HasFlag(Diacritic))
      "  ",
    end,
    sound.Text
  }

  let SoundSound(ref sound) begin
    if (sound == SpaceSound)
      return Space;
    end
    return sound.SymbolName
  end

  //======================================================================
  // Build a dictionary with SAMPA text as the key
  //======================================================================

  let GatherText(set, ref sound) begin
    if (sound.Sampa.Length > 0)
      set.AddElement(sound.Sampa, ref sound)
    end
  end

  let SampaDictionary begin
    var set = Type.Dictionary(128);
    GatherText(set, each Sounds);
    set.AddElement(" ", ref SpaceSound);
    return set;
  end

  // Convert SAMPA text to an array of sound references
  let WordToSounds(word) = SampaDictionary.FindTokens(word.Sampa, ref NoSound)

  let SoundToCharacter(ref sound) begin
    if (sound == GapSound)
      return {
        Character: Nexus.Gap,
        Sound: ref NoSound
      }
    end

    var f = Results.UniqueSounds.FindIndex(DisplayCharacters.SameSound, {Sound: ref sound});
    if (f.Length == 1)
      return {
        Character: Nexus.CharacterList[f[0]],
        Sound: ref sound
      }
    else
      return {
        Character: '?',
        Sound: ref NoSound
      }
    end
  end

end

