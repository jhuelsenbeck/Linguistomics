using Format, Units
//======================================================================

with IPA
  enum Opens
    Close,
    NearClose,
    CloseMid,
    Mid,
    OpenMid,
    NearOpen,
    Open
  end

  enum Backnesses
    Front,
    NearFront,
    Central,
    NearBack,
    Back,
  end

  enum Places
    Bilabial,
    Labial,
    LabialVelar,
    LabialPalatal,
    LabioDental,
    LinguoLabial,
    Dental,
    Alveolar,
    AlveoloPalatal,
    PostAlveolar,
    Retroflex,
    Palatal,
    PalatoAlveolar,
    Velar,
    Uvular,
    Pharyngeal,
    Glottal,
  end

  let Encode(text, sampa, alt=null) = {
    Popup: SoundPopup,
    Text: text,
    Sampa: sampa,
    Alt: alt
  }

  let Consonant(flags, place) = {
    Flags: flags,
    Place: place,
  }

  let Imp(flags, place) = {
    Flags: flags | Impossible,
    Place: place,
  }

  let Vowel(flags, open, backness) = {
    Flags: flags | IsVowel,
    Open: open,
    Backness: backness,
  }

  let NPEjective(flags, sound) = Encode(sound.Text Diacritics.Ejective.Text, sound.Sampa Diacritics.Ejective.Sampa) Consonant(flags | Ejective | Affricate, sound.Place)

  let None        = 0
  let Unrounded   = 0
  let Voiced      = 1 << 0
  let NonIPA      = 1 << 1
  let Rounded     = 1 << 2
  let Velarized   = 1 << 3
  let Ejective    = 1 << 4
  let Pulmonic    = 1 << 5
  let Nasal       = 1 << 6
  let Tenuis      = 1 << 7
  let Lateral     = 1 << 8
  let Sibilant    = 1 << 9
  let Fricative   = 1 << 10
  let Approximant = 1 << 11
  let Plosive     = 1 << 12
  let Implosive   = 1 << 13
  let Central     = 1 << 14
  let TapFlap     = 1 << 15
  let Trill       = 1 << 16
  let Stop        = 1 << 17
  let Click       = 1 << 18
  let Affricate   = 1 << 19
  let IsVowel     = 1 << 20
  let Impossible  = 1 << 21

  let MaxFlags = 21

  let FlagNames = [
    Lang.Voiced,
    "NonIPA",
    Lang.Rounded,
    "Velarized",
    Lang.Ejective,
    Lang.Pulmonic,
    "Nasal",
    "Tenuis",
    "Lateral",
    "Sibilant",
    "Fricative",
    "Approximant",
    "Plosive",
    "Implosive",
    "Central",
    "Tap/Flap",
    "Trill",
    "Stop",
    Lang.Click,
    "Affricate",
    "Vowel",
    "Impossible"
  ];

  with Sounds
    //==================================
    // Pulmonic Consonants
    //==================================

    // Nasal					
    let VlBilabialNasal           = Encode("m̥", "") Consonant(Pulmonic | Nasal, Places.Bilabial)
    let VdBilabialNasal           = Encode("m", "m") Consonant(Voiced | Pulmonic | Nasal, Places.Bilabial)
    let VdLabioDentalNasal        = Encode("ɱ", "F") Consonant(Voiced | Pulmonic | Nasal, Places.LabioDental)
    let VdLinguoLabioNasal        = Encode("n̼", "") Consonant(Voiced | Pulmonic | Nasal, Places.LinguoLabial)
    let VlAlveolarNasal           = Encode("n̥", "") Consonant(Pulmonic | Nasal, Places.Alveolar)
    let VdAlveolarNasal           = Encode("n", "n") Consonant(Voiced | Pulmonic | Nasal, Places.Alveolar)
    let VlRetroFlexNasal          = Encode("ɳ̊", "") Consonant(Pulmonic | Nasal, Places.Retroflex)
    let VdRetroFlexNasal          = Encode("ɳ", "n`") Consonant(Voiced | Pulmonic | Nasal, Places.Retroflex)
    let VlPalatalNasal            = Encode("ɲ̊", "") Consonant(Pulmonic | Nasal, Places.Palatal)
    let VdPalatalNasal            = Encode("ɲ", "J") Consonant(Voiced | Pulmonic | Nasal, Places.Palatal)
    let VlVelarNasal              = Encode("ŋ̊", "") Consonant(Pulmonic | Nasal, Places.Velar)
    let VdVelarNasal              = Encode("ŋ", "N") Consonant(Voiced | Pulmonic | Nasal, Places.Velar)
    let VdUvularNasal             = Encode("ɴ", "N\\") Consonant(Voiced | Pulmonic | Nasal, Places.Uvular)
														
    // Stop
    let VlBilabialStop            = Encode("p", "p") Consonant(Pulmonic | Stop, Places.Bilabial)
    let VdBilabialStop            = Encode("b", "b") Consonant(Voiced | Pulmonic | Stop, Places.Bilabial)
    let VlLabioDentalStop         = Encode("p̪", "") Consonant(Pulmonic | Stop, Places.LabioDental)
    let VdLabioDentalStop         = Encode("b̪", "") Consonant(Voiced | Pulmonic | Stop, Places.LabioDental)
    let VlLinguoLabialStop        = Encode("t̼", "") Consonant(Pulmonic | Stop, Places.LinguoLabial)
    let VdLinguoLabialStop        = Encode("d̼", "") Consonant(Voiced | Pulmonic | Stop, Places.LinguoLabial)
    let VlAlveolarStop            = Encode("t", "t") Consonant(Pulmonic | Stop, Places.Alveolar)
    let VdAlveolarStop            = Encode("d", "d") Consonant(Voiced | Pulmonic | Stop, Places.Alveolar)
    let VlRetroflexStop           = Encode("ʈ", "t`") Consonant(Pulmonic | Stop, Places.Retroflex)
    let VdRetroflexStop           = Encode("ɖ", "d`") Consonant(Voiced | Pulmonic | Stop, Places.Retroflex)
    let VlPalatalStop             = Encode("c", "c") Consonant(Pulmonic | Stop, Places.Palatal)
    let VdPalatalStop             = Encode("ɟ", "J\\") Consonant(Voiced | Pulmonic | Stop, Places.Palatal)
    let VlVelarStop               = Encode("k", "k") Consonant(Pulmonic | Stop, Places.Velar)
    let VdVelarStop               = Encode("ɡ", "g") Consonant(Voiced | Pulmonic | Stop, Places.Velar)
    let VlUvularStop              = Encode("q", "q") Consonant(Pulmonic | Stop, Places.Uvular)
    let VdUvularStop              = Encode("ɢ", "G\\") Consonant(Voiced | Pulmonic | Stop, Places.Uvular)
    let VdEpiglottalStop          = Encode("ʡ", ">\\") Consonant(Pulmonic | Stop, Places.Pharyngeal)
    let GlottalStop               = Encode("ʔ", "?") Consonant(Pulmonic | Stop, Places.Glottal)
		
    // Sibilant Fricative										
    let VlAlveolarSibFricative       = Encode("s", "s") Consonant(Pulmonic | Sibilant | Fricative, Places.Alveolar)
    let VdAlveolarSibFricative       = Encode("z", "z") Consonant(Voiced | Pulmonic | Sibilant | Fricative, Places.Alveolar)
    let VlPostalveolarSibFricative   = Encode("ʃ", "S") Consonant(Pulmonic | Sibilant | Fricative, Places.PostAlveolar)
    let VdPostalveolarSibFricative   = Encode("ʒ", "Z") Consonant(Voiced | Pulmonic | Sibilant | Fricative, Places.PostAlveolar)
    let VlRetroflexSibFricative      = Encode("ʂ", "s`") Consonant(Pulmonic | Sibilant | Fricative, Places.Retroflex)
    let VdRetroflexSibFricative      = Encode("ʐ", "z`") Consonant(Voiced | Pulmonic | Sibilant | Fricative, Places.Retroflex)
    let VlPalatalSibFricative        = Encode("ɕ", "s\\") Consonant(Pulmonic | Sibilant | Fricative, Places.Palatal)
    let VdPalatalSibFricative        = Encode("ʑ", "z\\") Consonant(Voiced | Pulmonic | Sibilant | Fricative, Places.Palatal)

    // Fricative										
    let VlBilabialFricative          = Encode("ɸ", "p\\") Consonant(Pulmonic | Fricative, Places.Bilabial)
    let VdBilabialFricative          = Encode("β", "B") Consonant(Voiced | Pulmonic|  Fricative, Places.Bilabial)
    let VlLabiodentalFricative       = Encode("f", "f") Consonant(Pulmonic | Fricative, Places.LabioDental)
    let VdLabiodentalFricative       = Encode("v", "v") Consonant(Voiced | Pulmonic | Fricative, Places.LabioDental)
    let VlLinguoLabialFricative      = Encode("θ̼", "") Consonant(Pulmonic | Fricative, Places.LinguoLabial)
    let VdLinguoLabialFricative      = Encode("ð̼", "") Consonant(Voiced | Pulmonic | Fricative, Places.LinguoLabial)
    let VlDentalFricative            = Encode("θ", "T") Consonant(Pulmonic | Fricative, Places.Dental)
    let VdDentalFricative            = Encode("ð", "D") Consonant(Voiced | Pulmonic | Fricative, Places.Dental)
    let VlAlveolarFricative          = Encode("θ̠", "") Consonant(Pulmonic | Fricative, Places.Alveolar)
    let VdAlveolarFricative          = Encode("ð̠", "") Consonant(Voiced | Pulmonic | Fricative, Places.Alveolar)
    let VlPostaveolarFricative       = Encode("ɹ̠̊", "") Consonant(Pulmonic | Fricative, Places.PostAlveolar)
    let VdPostalveolarFricative      = Encode("ɹ̝̊", "") Consonant(Voiced | Pulmonic | Fricative, Places.PostAlveolar)
    let VdRetroflexFricative         = Encode("ɻ˔","") Consonant(Voiced | Pulmonic | Fricative, Places.Retroflex)
    let VlPalatalFricative           = Encode("ç", "C") Consonant(Pulmonic | Fricative, Places.Palatal)
    let VdPalatalFricative           = Encode("ʝ", "j\\") Consonant(Voiced | Pulmonic | Fricative, Places.Palatal)
    let VlVelarFricative             = Encode("x", "x") Consonant(Pulmonic | Fricative, Places.Velar)
    let VdVelarFricative             = Encode("ɣ", "G") Consonant(Voiced | Pulmonic | Fricative, Places.Velar)
    let VlUvularFricative            = Encode("χ", "X") Consonant(Pulmonic | Fricative, Places.Uvular)
    let VdUvularFricative            = Encode("ʁ", "R") Consonant(Voiced | Pulmonic | Fricative, Places.Uvular)
    let VlPharyngealFricative        = Encode("ħ", "X\\") Consonant(Pulmonic | Fricative, Places.Pharyngeal)
    let VdPharyngealFricative        = Encode("ʕ", "?\\") Consonant(Voiced | Pulmonic | Fricative, Places.Pharyngeal)
    let VlGlottalFricative           = Encode("h", "h") Consonant(Pulmonic | Fricative, Places.Glottal)
    let VdGlottalFricative           = Encode("ɦ", "h\\") Consonant(Voiced | Pulmonic | Fricative, Places.Glottal)
														
    // Approximant										
    let VlLabioDentalApproximant     = Encode("ʋ̥", "") Consonant(Pulmonic | Approximant, Places.LabioDental)
    let VdLabioDentalApproximant     = Encode("ʋ", "v\\", "P") Consonant(Voiced | Pulmonic | Approximant, Places.LabioDental)
    let VlPostalveolarApproximant    = Encode("ɹ̥", "") Consonant(Pulmonic | Approximant, Places.Alveolar)
    let VdPostalveolarApproximant    = Encode("ɹ", "r\\") Consonant(Voiced | Pulmonic | Approximant, Places.Alveolar)
    let VlRetroflexApproximant       = Encode("ɻ̊", "") Consonant(Pulmonic | Approximant, Places.Retroflex)
    let VdRetroflexApproximant       = Encode("ɻ", "r\\`") Consonant(Voiced | Pulmonic | Approximant, Places.Retroflex)
    let VlPalatalApproximant         = Encode("j̊", "") Consonant(Pulmonic | Approximant, Places.Palatal)
    let VdPalatalApproximant         = Encode("j", "j") Consonant(Voiced | Pulmonic | Approximant, Places.Palatal)
    let VlVelarApproximant           = Encode("ɰ̊", "") Consonant(Pulmonic | Approximant, Places.Velar)
    let VdVelarApproximant           = Encode("ɰ", "M\\") Consonant(Voiced | Pulmonic | Approximant, Places.Velar)
    let VdGlottalApproximant         = Encode("ʔ̞", "") Consonant(Voiced | Pulmonic | Approximant, Places.Glottal)
														
    // Tap or Flap										
    let VdBilabialDentalFlap         = Encode("ⱱ̟", "") Consonant(Voiced | Pulmonic | TapFlap, Places.Bilabial)
    let VdLabioDentalFlap            = Encode("ⱱ", "") Consonant(Voiced | Pulmonic | TapFlap, Places.LabioDental)
    let VdLingualLabialStop          = Encode("ɾ̼", "") Consonant(Voiced | Pulmonic | TapFlap, Places.LinguoLabial)
    let VlAlveolarFlap               = Encode("ɾ̥", "") Consonant(Pulmonic | TapFlap, Places.Alveolar)
    let VdAlveolarTap                = Encode("ɾ", "4") Consonant(Voiced | Pulmonic | TapFlap, Places.Alveolar)
    let VlRetroflexFlap              = Encode("ɽ̊", "") Consonant(Pulmonic | TapFlap, Places.Retroflex)
    let VdRetroflexFlap              = Encode("ɽ", "r`") Consonant(Voiced | Pulmonic | TapFlap, Places.Retroflex)
    let VdUvularFlap                 = Encode("ɢ̆", "") Consonant(Voiced | Pulmonic | TapFlap, Places.Uvular)
    let VdPharyngealFlap             = Encode("ʡ̆", "") Consonant(Voiced | Pulmonic | TapFlap, Places.Pharyngeal)
														
    // Trill											
    let VlBilabialTrill              = Encode("ʙ̥", "") Consonant(Pulmonic | Trill, Places.Bilabial)
    let VdBilabialTrill              = Encode("ʙ", "B\\") Consonant(Voiced | Pulmonic | Trill, Places.Bilabial)
    let VlAlveolarTrill              = Encode("r̥", "") Consonant(Pulmonic | Trill, Places.Alveolar)
    let VdAlveolarTrill              = Encode("r", "r") Consonant(Voiced | Pulmonic | Trill, Places.Alveolar)
    let VlRetroflexTrill             = Encode("ɽ̊r̥", "") Consonant(Pulmonic | Trill, Places.Retroflex)
    let VdRetroflexTrill             = Encode("ɽr", "") Consonant(Voiced | Pulmonic | Trill, Places.Retroflex)
    let VlUvularTrill                = Encode("ʀ", "R\\") Consonant(Pulmonic | Trill, Places.Uvular)
    let VlPharyngealTrill            = Encode("H", "") Consonant(Pulmonic | Trill, Places.Pharyngeal)
    let VdPharyngealTrill            = Encode("ʢ", "<\\") Consonant(Voiced | Pulmonic | Trill, Places.Pharyngeal)
														
    // Lateral Fricative								
    let VlAlveolarLateralFricative   = Encode("ɬ", "K") Consonant(Pulmonic | Lateral | Fricative, Places.Alveolar)
    let VdAlveolarLateralFricative   = Encode("ɮ", "K\\") Consonant(Voiced | Pulmonic | Lateral | Fricative, Places.Alveolar)
    let VlRetroflexLateralFricative  = Encode("ɭ̊˔", "") Consonant(Pulmonic | Lateral | Fricative, Places.Retroflex)
    let VdRetroflexLateralFricative  = Encode("ɭ˔", "") Consonant(Voiced | Pulmonic | Lateral | Fricative, Places.Retroflex)
    let VlAlveolarPalatalFricative   = Encode("ʎ̝̊", "") Consonant(Pulmonic | Lateral | Fricative, Places.Palatal)
    let VdAlveolarPalatalFricative   = Encode("ʎ̝", "") Consonant(Voiced | Pulmonic | Lateral | Fricative, Places.Palatal)
    let VlVelarPalatalFricative      = Encode("ʟ̝̊", "") Consonant(Pulmonic | Lateral | Fricative, Places.Velar)
    let VdVelarPalatalFricative      = Encode("ʟ̝", "") Consonant(Voiced | Pulmonic | Lateral | Fricative, Places.Velar)
														
    // LateralApproximant								
    let VlAlveolarLateralApproximant = Encode("l̥", "") Consonant(Pulmonic | Lateral | Approximant, Places.Alveolar)
    let VdAlveolarLateralApproximant = Encode("l", "l") Consonant(Voiced | Pulmonic | Lateral | Approximant, Places.Alveolar)
    let VdVelarizedAlveolarLateralApproximant = Encode("ɫ", "5") Consonant(Voiced | Velarized | Pulmonic | Lateral | Approximant, Places.Alveolar)
    let VlRetroflexLateral           = Encode("ɭ̊", "") Consonant(Pulmonic | Lateral | Approximant, Places.Retroflex)
    let VdRetroflexLateral           = Encode("ɭ", "n`") Consonant(Voiced | Pulmonic | Lateral | Approximant, Places.Retroflex)
    let VlPalatalLateral             = Encode("ʎ̥", "") Consonant(Pulmonic | Lateral | Approximant, Places.Palatal)
    let VdPalatalLateral             = Encode("ʎ", "L") Consonant(Voiced | Pulmonic | Lateral | Approximant, Places.Palatal)
    let VlVelarLateral               = Encode("ʟ̥", "") Consonant(Pulmonic | Lateral | Approximant, Places.Velar)
    let VdVelarLateral               = Encode("ʟ", "L\\") Consonant(Voiced | Pulmonic | Lateral | Approximant, Places.Velar)
    let VdUvularLateral              = Encode("ʟ̠", "") Consonant(Voiced | Pulmonic | Lateral | Approximant, Places.Uvular)

    // Lateral tap/flap
    let VdAlveolarLateralFlap        = Encode("ɺ", "l\\") Consonant(Voiced | Pulmonic | Lateral | TapFlap, Places.Alveolar)
    let VdRetroflexLateralFlap       = Encode("ɭ̆", "") Consonant(Voiced | Pulmonic | Lateral | TapFlap, Places.Retroflex)
    let VdPalatalLateralFlap         = Encode("ʎ̆", "") Consonant(Voiced | Pulmonic | Lateral | TapFlap, Places.Palatal)
    let VdVelarLateralTap            = Encode("L̆", "") Consonant(Voiced | Pulmonic | Lateral | TapFlap, Places.Velar)


    //==================================
    // Non-Pulmonic Consonants
    //==================================

    // Stops
    let VlBilabialStopEjective       = NPEjective(Stop, VlBilabialStop)
    let VlAveolarStopEjective        = NPEjective(Stop, VlAlveolarStop)
    let VlRetroflexStopEjective      = NPEjective(Stop, VlRetroflexStop)
    let VlPalatalStopEjective        = NPEjective(Stop, VlPalatalStop)
    let VlVelarStopEjective          = NPEjective(Stop, VlVelarStop)
    let VlUvularStopEjective         = NPEjective(Stop, VlUvularStop)
    let VlEpiglottalStopEjective     = NPEjective(Stop, VdEpiglottalStop)

    // Fricatives
    let VlBilabialFricativeEjective     = NPEjective(Fricative, VlBilabialFricative)
    let VlLabiodentalFricativeEjective  = NPEjective(Fricative, VlLabiodentalFricative)
    let VlDentalFricativeEjective       = NPEjective(Fricative, VlDentalFricative)
    let VlAlveolarFricativeEjective     = NPEjective(Fricative, VlAlveolarSibFricative)
    let VlPostalveolarFricativeEjective = NPEjective(Fricative, VlPostalveolarSibFricative)
    let VlRetroflexFricativeEjective    = NPEjective(Fricative, VlRetroflexSibFricative)
    let VlPalatalFricativeEjective      = NPEjective(Fricative, VlPalatalSibFricative)
    let VlVelarFricativeEjective        = NPEjective(Fricative, VlVelarFricative)
    let VlUvularFricativeEjective       = NPEjective(Fricative, VlUvularFricative)

    // Lateral Fricatives
    let VlPostalveolarLatFricEjective = NPEjective(Lateral | Fricative, VlAlveolarLateralFricative)

    // Clicks
    let VlBilabialTenuisClick        = Encode("ʘ", "O\\") Consonant(Ejective | Tenuis | Click | Affricate, Places.Bilabial)
    let VdBilabialTenuisClick        = Encode("ʘ̬", "") Consonant(Voiced | Ejective | Tenuis | Click | Affricate, Places.Bilabial)
    let VlDentalTenuisClick          = Encode("ǀ", " | ") Consonant(Ejective | Tenuis | Click | Affricate, Places.Dental)
    let VdDentalTenuisClick          = Encode("ǀ̬", "") Consonant(Voiced | Ejective Tenuis | Click | Affricate, Places.Dental)
    let VlAlveolarTenuisClick        = Encode("ǃ", "!\\") Consonant(Ejective | Tenuis | Click | Affricate, Places.Alveolar)
    let VdAlveolarTenuisClick        = Encode("ǃ̬", "") Consonant(Voiced | Ejective | Tenuis | Click | Affricate, Places.Alveolar)
    let VlPalatalTenuisClick         = Encode("ǂ", "=\\") Consonant(Ejective | Tenuis | Click | Affricate, Places.Palatal)
    let VdPalatalTenuisClick         = Encode("ǂ̬", "") Consonant(Voiced | Ejective | Tenuis | Click | Affricate, Places.Palatal)

    let VlBilabialNasalClick         = Encode("ʘ̃", "") Consonant(Ejective | Nasal | Click | Affricate, Places.Bilabial)
    let VlDentalNasalClick           = Encode("ǀ̃", "") Consonant(Ejective | Nasal | Click | Affricate, Places.Dental)
    let VlAlveolarNasalClick         = Encode("ǃ̃", "") Consonant(Ejective | Nasal | Click | Affricate, Places.Alveolar)
    let VlPalatalNasalClick          = Encode("ǂ̃", "") Consonant(Ejective | Nasal | Click | Affricate, Places.Palatal)

    let VlAlveolarTenuisLateralClick = Encode("ǁ", " | \\ | \\") Consonant(Ejective | Tenuis | Lateral | Click | Affricate, Places.Alveolar)
    let VdAlveolarTenuisLateralClick = Encode("ǁ̬", "") Consonant(Voiced | Ejective | Tenuis | Lateral | Click | Affricate, Places.Alveolar)

    let VlBilabialImplosiveClick     = Encode("ɓ̥", "") Consonant(Ejective | Implosive | Click | Affricate, Places.Bilabial)
    let VdBilabialImplosiveClick     = Encode("ɓ", "b_<") Consonant(Voiced | Ejective | Implosive | Click | Affricate, Places.Bilabial)
    let VlAlveolarImplosiveClick     = Encode("ɗ̥", "") Consonant(Ejective | Implosive | Click | Affricate, Places.Alveolar)
    let VdAlveolarImplosiveClick     = Encode("ɗ", "d_<") Consonant(Voiced | Ejective | Implosive | Click | Affricate, Places.Alveolar)
    let VlRetroflexImplosiveClick    = Encode("ᶑ̊", "") Consonant(Ejective | Implosive | Click | Affricate, Places.Retroflex)
    let VdRetroflexImplosiveClick    = Encode("ᶑ", "") Consonant(Voiced | Ejective | Implosive | Click | Affricate, Places.Retroflex)
    let VlPalatalImplosiveClick      = Encode("ʄ̊", "") Consonant(Ejective | Implosive | Click | Affricate, Places.Palatal)
    let VdPalatalImplosiveClick      = Encode("ʄ", "J\\_<") Consonant(Voiced | Ejective | Implosive | Click | Affricate, Places.Palatal)
    let VlVelarImplosiveClick        = Encode("ɠ̊", "") Consonant(Ejective | Implosive | Click | Affricate, Places.Velar)
    let VdVelarImplosiveClick        = Encode("ɠ", "g_<") Consonant(Voiced | Ejective | Implosive | Click | Affricate, Places.Velar)
    let VlUvularImplosiveClick       = Encode("ʛ̥", "") Consonant(Ejective | Implosive | Click | Affricate, Places.Uvular)
    let VdUvularImplosiveClick       = Encode("ʛ", "G\\_<") Consonant(Voiced | Ejective | Implosive | Click | Affricate, Places.Uvular)

    //==================================
    // Pulmonic | Affricates
    //==================================

    // Sibilants
    let VlAlveolarAffricate          = Encode("ts", "") Consonant(Pulmonic | Sibilant | Affricate, Places.Alveolar)
    let VdAlveolarAffricate          = Encode("dz", "") Consonant(Voiced | Pulmonic | Sibilant | Affricate, Places.Alveolar)
    let VlPostalveolarAffricate      = Encode("t̠ʃ", "") Consonant(Pulmonic | Sibilant | Affricate, Places.PalatoAlveolar)
    let VdPostalveolarAffricate      = Encode("d̠ʒ", "") Consonant(Voiced | Pulmonic | Sibilant | Affricate, Places.PalatoAlveolar)
    let VlRetroflexAffricate         = Encode("ʈʂ", "") Consonant(Pulmonic | Sibilant | Affricate, Places.Retroflex)
    let VdRetroflexAffricate         = Encode("ɖʐ", "") Consonant(Voiced | Pulmonic | Sibilant | Affricate, Places.Retroflex)
    let VlAlveoloPalatalAffricate    = Encode("tɕ", "") Consonant(Pulmonic | Sibilant | Affricate, Places.AlveoloPalatal)
    let VdAlveoloPalatalAffricate    = Encode("dʑ", "") Consonant(Voiced | Pulmonic | Sibilant | Affricate, Places.AlveoloPalatal)

    // Non-Sibilants
    let VlBilabialNSAffricate        = Encode("pɸ", "") Consonant(Pulmonic | Affricate, Places.Bilabial)
    let VdBilabialNSAffricate        = Encode("bβ", "") Consonant(Voiced | Pulmonic | Affricate, Places.Bilabial)
    let VlLabioDentalNSAffricate     = Encode("p̪f", "") Consonant(Pulmonic | Affricate, Places.LabioDental)
    let VdLabioDentalNSAffricate     = Encode("b̪v", "") Consonant(Voiced | Pulmonic | Affricate, Places.LabioDental)
