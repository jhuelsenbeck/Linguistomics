class EuropeBrazilExperimentClass: DataSetClass {
  Constructor(config) {
    super.Constructor(config, "Europe and Brazil", "EuropeB", Info.RunIndex);
  }

  override NodeClass PortugueseClade = new NodeClass {
    new Leaf(Portuguese);
    new Leaf(PortugueseB);
  };

  override NodeClass PortugueseNumbered = new NBranch(12) {
    new NLeaf(Portuguese, 1);
    new NLeaf(PortugueseB, 1);
  };
}

